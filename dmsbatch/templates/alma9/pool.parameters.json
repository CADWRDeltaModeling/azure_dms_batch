{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "batchAccountName": {
            "value": "batch_account_name"
        },
        "storageAccountName": {
            "value": "storage_account_name"
        },
        "storageAccountKey": {
            "value": "storage_account_key"
        },
        "storageContainerName": {
            "value": "storage_container_name"
        },
        "poolName": {
            "value": "pool_name"
        },
        "vmSize": {
            "value": "Standard_HB120rs_v2"
        },
        "taskSlotsPerNode": {
            "value": 1
        },
        "nodeAgentSKUId": {
            "value": "batch.node.el 9"
        },
        "startTaskScript": {
            "value": "printenv && mkdir -p /opt/schism_scripts && cp -r $AZ_BATCH_NODE_MOUNTS_DIR/dwrbdoschismsa/batch/schism_scripts /opt/schism_scripts && chmod +x /opt/schism_scripts/* && /opt/schism_scripts/start_task.sh"
        },
        "formula": {
            "value": "formula"
        },
        "appInsightsInstrumentationKey": {
            "value": "app_insights_instrumentation_key"
        },
        "appInsightsAppId": {
            "value": "app_insights_app_id"
        }
    }
}